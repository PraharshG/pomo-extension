{
  "manifest_version": 3,
  "name": "Study Assistant",
  "version": "1.0",
  "description": "A Chrome extension for studying efficiently",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}